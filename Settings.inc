[Variables]
style=classic

size=70
;icon size

space=0
;space between icon, if other than 0 the icon will fix in their position

background=0,0,0,150
;Background color, delete this line remove background

;background_width=60

divider=4
;how slow animation calculation should be (increase to make animation longer)

animspd_init=150
animspd_enter=30
animspd_exit=20
;Speed for various animation

update_rate=15
;Redraw delay (framerate delay)

step=24
;how many update to draw when interacted (increase for longer animation)

expand=2
;how big icon expand when highlighted (size multiplier)

padding=15
;increase to make icon smaller

focus=0
;Whether to hide icons when a submenu is open

autohide=0
;Whether to hide icon after period of time

timeout=1500
;Timeout for autohide

cooldown=1000
; Cooldown after click on an icon

direction=1
;direction of the skin, 1:up  2:down  3:left  4:right


FontColor=255,255,255
FontSize=20
;Color and size icon name